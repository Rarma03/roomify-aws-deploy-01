{
    "buildCommand": "yarn install && yarn build",
    "outputDirectory": "dist",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        }
    ],
    "routes": [
        {
            "src": "/api/",
            "dest": "/"
        }
    ]
}