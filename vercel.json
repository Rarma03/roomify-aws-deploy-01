{
    "buildCommand": "yarn install && yarn build",
    "outputDirectory": "dist",
    "framework": "vite",
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.js"
        },
        {
            "src": "/api",
            "status": 301,
            "headers": {
                "Location": "/api/"
            }
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}